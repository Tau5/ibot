<!DOCTYPE html>
<html lang="en">
  <head>
    <% include parts/head.ejs %>
    <title>iBot - Commands</title>
  </head>
  <body>
    <!-- Nav-bar -->
    <% include parts/header.ejs %>

    <!-- Commands -->
    <div class="jumbotron jumbotron-fluid">
      <div class="container">
        <h1 class="mbr-section-title mbr-bold mbr-fonts-style text-center"><strong>INTRODUCING IBOT</strong></h1>
        <hr>
        <% Object.keys(client.help).forEach((command) => {
        <div aria-label="breadcrumb" role="navigation">
          <strong>Command:</strong> i:<%= command %><br>
          <strong>Description:</strong> i:<%= client.help[command].description %><br>
          <strong>Usage:</strong> <%= client.help[command].usage %>
        </div>
      </div>
    </div>

    <!-- Footer -->
    <% include parts/footer.ejs %>
  </body>
</html>