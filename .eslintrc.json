{
    "extends": "airbnb-base",
    "rules": {
        "no-console": 0,
        "global-require": 0,
        "import/no-dynamic-require": 0,
        "consistent-return": 0,
        "no-eval": 0,
        "no-param-reassign": 0,
        "no-restricted-globals": 0,
        "prefer-destructuring": 0,
        "radix": 0,
        "no-unused-vars": 0,
        "max-len": 0
    }
}